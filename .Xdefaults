URxvt.scrollBar:            false
URxvt.font:                 xft:xos4 Terminus:size=18:antialias=False,xft:FontAwesome:antialias=false
URxvt.letterSpace:          1
#URxvt.termName:             screen-256color
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,fullscreen,resize-font,selection-autotransform
URxvt.url-select.launcher:  chromium
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.keysym.F11:           perl:fullscreen:switch
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755:             false
URxvt.resize-font.smaller:  C-Down
URxvt.resize-font.bigger:   C-Up
URxvt.selection-autotransform.0: s/ +$//gm
